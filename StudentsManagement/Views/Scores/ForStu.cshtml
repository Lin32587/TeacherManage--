@model IEnumerable<StudentsManagement.Models.Score>

@{
    ViewBag.Title = "教师上课信息";
    Layout = "~/Views/Shared/_LayoutForStu.cshtml";
}

<h3>课程信息</h3>
<table class="table" style="margin-top:80px">
    <tr class="justify-content-lg-center">
        <th>
            @Html.DisplayNameFor(model => model.Course.CName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Course.CName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestTime)
            </td>
        </tr>
    }

</table>

<a class="btn btn-dark" href="@Url.Action("ForStu","Lin")">Back</a>